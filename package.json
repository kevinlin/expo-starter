{
  "name": "stickersmash",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "ts:check": "tsc",
    "analyze:ios": "source-map-explorer 'dist/_expo/static/js/ios/*.js' 'dist/_expo/static/js/ios/*.js.map'",
    "storybook": "STORYBOOK_ENABLED=1 expo start",
    "storybook:ios": "STORYBOOK_ENABLED=1 expo start --ios",
    "storybook:android": "STORYBOOK_ENABLED=1 expo start --android",
    "analyze:android": "source-map-explorer 'dist/_expo/static/js/android/*.js' 'dist/_expo/static/js/android/*.js.map'",
    "lint": "eslint 'app/**/*.{ts,tsx}'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "app/**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.10.4",
    "@expo/metro-runtime": "~3.2.1",
    "@expo/vector-icons": "^14.0.0",
    "@gluestack-style/react": "^1.0.56",
    "@gluestack-ui/config": "^1.1.18",
    "@gluestack-ui/themed": "^1.1.26",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/datetimepicker": "^8.1.0",
    "@react-native-community/slider": "^4.5.2",
    "@storybook/addon-ondevice-actions": "^7.6.19",
    "@storybook/addon-ondevice-controls": "^7.6.19",
    "@storybook/react-native": "^7.6.19",
    "expo": "~51.0.8",
    "expo-constants": "~16.0.1",
    "expo-dev-client": "~4.0.15",
    "expo-image-picker": "~15.0.5",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-media-library": "~16.0.3",
    "expo-router": "~3.5.14",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "graphql": "^15.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-localization": "^1.0.19",
    "react-native": "0.74.1",
    "react-native-gesture-handler": "~2.16.1",
